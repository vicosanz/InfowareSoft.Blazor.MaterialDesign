@namespace InfowareSoft.Blazor.MaterialDesign.Components
@inherits DOMComponent

<As Tag="@Tag.GetName()" Class="@ClassMapper.Class">@ChildContent</As>

@code {
    protected override void OnInitialized()
    {
        base.OnInitialized();
        ClassMapper.Variant(() => Variant.GetName());
    }

    [Parameter] public MDTypoAs Tag { get; set; } = MDTypoAs.h1;
    [Parameter] public MDTypoVariant Variant { get; set; } = MDTypoVariant.HeadLine1;
}
