@code{
    private bool Exited = false;

    private bool Toogle1 = false;

    private void ExitedFab()
    {
        Exited = !Exited;
    }

    private void ClickToogle1()
    {
        Toogle1 = !Toogle1;
    }

    private bool DialogIsOpen = false;
    private void OpenDialog(bool value)
    {
        Console.WriteLine("opendialog");
        DialogIsOpen = value;
        StateHasChanged();
    }

    private void OnDialogOpenChange(bool value)
    {
        DialogIsOpen = value;
        StateHasChanged();
    }
}
